{
  "name": "agent-runtime",
  "version": "0.1.0",
  "description": "AI Agent Runtime for Tauri Personal Assistant",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-runtime": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build:sidecar": "npm run build && pkg dist/index.js --targets node18-linux-arm64,node18-linux-x64,node18-macos-arm64,node18-macos-x64,node18-win-x64 --output ../src-tauri/binaries/agent-runtime",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "@yao-pkg/pkg": "^5.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
