{
  "name": "agent-runtime",
  "version": "0.1.0",
  "description": "AI Agent Runtime for Tauri Personal Assistant",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-runtime": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build:sidecar": "npm run build && pkg dist/index.js --targets node18-linux-arm64,node18-linux-x64,node18-macos-arm64,node18-macos-x64,node18-win-x64 --output ../src-tauri/binaries/agent-runtime"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@yao-pkg/pkg": "^5.0.0"
  }
}
